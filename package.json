    "istanbul": "^0.4.5",
    "jsdoc": "^3.4.0",
    "mocha": "^2.2.4",
    "node-mocks-http": "^1.5.4",
    "noisegen": "^1.0.0",
    "proxyquire": "^1.7.3",
    "sinon": "^1.14.1"
  }
}
